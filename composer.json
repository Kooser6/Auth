{
    "name": "kooser/auth",
    "description": "Create complex authentication systems with complete ease.",
    "homepage": "https://kooser6.github.io/Auth/",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "paragonie/easydb": "^2",
        "kooser/session": "^2.0",
        "kooser/password-lock": "^1.0",
        "symfony/options-resolver": "^4.3",
